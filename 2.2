#include <iostream>
using namespace std;

int main()
{
    int a = 0;
    int b = 0;

    cout << "Podaj liczbe a: ";
    cin >> a;
    cout << "Podaj liczbe b: ";
    cin >> b;
    cout << endl;

    cout << "Pobrano liczbe a: "<< a << endl;
    cout << "Pobrano liczbe b: "<< b << endl;
